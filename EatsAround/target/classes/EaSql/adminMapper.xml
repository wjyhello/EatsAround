<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.eatsaround.EaSql.adminMapper">
<select id="getMemberList" resultType="com.eatsaround.vo.MemberVO">
    SELECT userId, userName, userEmail, userPhone, regDate, verify
    FROM ea_member
</select>

<delete id="deleteMember">
<![CDATA[
DELETE FROM ea_member WHERE userId = #{userId}
]]>
</delete>

</mapper>